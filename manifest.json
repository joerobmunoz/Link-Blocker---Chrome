{
  "name": "Link Blocker",
  "description": "Allows user to set responsible time limits on websites.",
  "manifest_version": 2,

  "version": "1.0",

  "background": {
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": "img/sunglasses-icon-16.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": { 
    "128": "img/sunglasses-icon-128.png"
  }
}