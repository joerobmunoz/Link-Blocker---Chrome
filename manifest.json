{
  "name": "Link Blocker",
  "description": "Allows user to set responsible time limits on websites.",
  "manifest_version": 2,

  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "sunglasses-icon-16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webNavigation"
  ],
  "icons": { 
    "128": "sunglasses-icon-128.png"
  }
}